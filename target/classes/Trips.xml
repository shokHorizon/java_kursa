<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="Entity.Trips">
        <table name="trips" schema="kursa" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="tour">
                <column name="tour" precision="-1"/>
            </basic>
            <basic name="price">
                <column name="price" precision="-1"/>
            </basic>
            <basic name="date">
                <column name="date" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
