<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<SplitPane dividerPositions="0.2" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Controllers.adminController">
   <items>
      <VBox fx:id="vbox" alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="15.0">
         <children>
            <ComboBox fx:id="combo1" editable="true" onAction="#combo1Action" prefWidth="150.0" />
            <ComboBox fx:id="combo2" editable="true" onAction="#combo2Action" prefWidth="150.0" />
            <ComboBox fx:id="combo3" editable="true" onAction="#combo3Action" prefWidth="150.0" />
            <ComboBox fx:id="combo4" editable="true" onAction="#combo4Action" prefWidth="150.0" />
            <ComboBox fx:id="combo5" editable="true" onAction="#combo5Action" prefWidth="150.0" />
            <ComboBox fx:id="combo6" editable="true" onAction="#combo6Action" prefWidth="150.0" />
            <ComboBox fx:id="combo7" editable="true" onAction="#combo7Action" prefWidth="150.0" />
            <Button fx:id="btnAdd" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnAddClick" style="-fx-background-color: #aaffaa;" text="Добавить" />
            <Button fx:id="btnUpdate" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnUpdateClick" style="-fx-background-color: #ffffaa;" text="Изменить" />
            <Button fx:id="btnRemove" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#btnRemoveClick" style="-fx-background-color: #ffaaaa;" text="Удалить" />
         </children>
      </VBox>
      <TabPane fx:id="tabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab fx:id="tabTravel" onSelectionChanged="#travelsTabSelected" text="Travels">
               <content>
                  <TableView fx:id="tableTravels" onMouseClicked="#travelsClick" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="travelsId" prefWidth="75.0" text="type" />
                        <TableColumn fx:id="travelsCity" prefWidth="75.0" text="city" />
                        <TableColumn fx:id="travelsName" prefWidth="75.0" text="name" />
                        <TableColumn fx:id="travelsUser" prefWidth="75.0" text="user" />
                        <TableColumn fx:id="travelsPrice" prefWidth="75.0" text="price" />
                        <TableColumn fx:id="travelsCoordinates" prefWidth="75.0" text="coordinates" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </Tab>
          <Tab fx:id="tabTickets" onSelectionChanged="#ticketsTabSelected" text="Tickets">
               <content>
                  <TableView fx:id="tableTickets" onMouseClicked="#ticketsClick" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="ticketsId" prefWidth="75.0" text="id" />
                        <TableColumn fx:id="ticketsTravel" prefWidth="75.0" text="travel" />
                      <TableColumn fx:id="ticketsUser" prefWidth="75.0" text="user" />
                        <TableColumn fx:id="ticketsStatus" prefWidth="75.0" text="status" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </Tab>
            <Tab fx:id="tabUsers" onSelectionChanged="#usersTabSelected" text="Users">
               <content>
                  <TableView fx:id="tableUsers" onMouseClicked="#usersClick" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="usersId" prefWidth="75.0" text="id" />
                      <TableColumn fx:id="usersLogin" prefWidth="75.0" text="login" />
                        <TableColumn fx:id="usersHashedPassword" prefWidth="75.0" text="hashedPassword" />
                        <TableColumn fx:id="usersAccessLevel" prefWidth="75.0" text="accessLevel" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </Tab>
            <Tab fx:id="tabCities" onSelectionChanged="#citiesTabSelected" text="Cities">
               <content>
                  <TableView fx:id="tableCities" onMouseClicked="#citiesClick" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="citiesId" prefWidth="75.0" text="id" />
                      <TableColumn fx:id="citiesName" prefWidth="75.0" text="name" />
                        <TableColumn fx:id="citiesCountry" prefWidth="75.0" text="country" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </Tab>
            <Tab fx:id="tabCountries" onSelectionChanged="#countriesTabSelected" text="Countries">
               <content>
                  <TableView fx:id="tableCountries" onMouseClicked="#countriesClick" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                      <TableColumn fx:id="countriesId" prefWidth="75.0" text="id" />
                      <TableColumn fx:id="countriesName" prefWidth="75.0" text="name" />
                    </columns>
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </items>
</SplitPane>
